@using WebTest.Models
@model IEnumerable<PupilSession>    

<div class="cointeiner p-3">
    <div class="row p-4">
        <div class="col-6">
            <h2 class="text-primary">Answer list</h2>
        </div>
        
    </div>
    <br />

<table class ="table table-bordered table-striped" style="width:100%">
<thead>
    <tr>
        <th>
            Number question
        </th>    
        <th>
           Answer
        </th>
        
        
    </tr>

</thead>
<tbody>

    @foreach(var obj in Model)
    {
        <tr>
            <td width="50%">
                <a  >@obj.QuestionNumber</a>
                
            </td>
            <td width="30%">
                @Html.Raw(@obj.Answer)
            </td>            
           
                
            
        </tr>
    }

    

</tbody>
</table>
</div>
